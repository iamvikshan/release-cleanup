{
  "name": "git-release-cleanup",
  "description": "Automatically delete releases across Github and GitLab",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && ncc build src/index.ts -o dist --source-map",
    "lint": "eslint src/**/*.ts",
    "f": "prettier --write \"**/*.*\"",
    "prepare": "husky",
    "test": "jest"
  },
  "keywords": [
    "github",
    "rleases",
    "tags",
    "automation",
    "gitlab"
  ],
  "author": "vikshan",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.6",
    "@eslint/js": "^9.15.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vercel/ncc": "^0.38.3",
    "@zeit/ncc": "^0.22.3",
    "axios": "^1.7.7",
    "bun-types": "^1.1.36",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "conventional-changelog-gitmoji": "^0.2.5",
    "eslint": "^9.15.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.0",
    "semantic-release-gitmoji": "^1.6.8",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-require-imports": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "error"
    }
  }
}