{
  "name": "git-release-cleanup",
  "description": "Automatically delete releases, tags, and Docker images across GitHub, GitLab, and container registries",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && ncc build src/index.ts -o dist --source-map",
    "lint": "eslint src/**/*.ts",
    "f": "prettier --write \"**/*.*\"",
    "prepare": "husky",
    "test": "jest",
    "start": "bun src/index.ts"
  },
  "keywords": [
    "github",
    "releases",
    "tags",
    "automation",
    "gitlab",
    "docker",
    "ghcr",
    "container-registry",
    "cleanup"
  ],
  "author": "vikshan",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "inquirer": "^8.2.7"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@eslint/js": "^9.39.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.0.0",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vercel/ncc": "^0.38.4",
    "bun-types": "^1.3.3",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "conventional-changelog-gitmoji": "^0.2.5",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.0",
    "semantic-release-gitmoji": "^1.6.8",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-require-imports": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "error"
    }
  }
}
